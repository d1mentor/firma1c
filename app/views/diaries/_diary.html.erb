      <tr class="table-success" name="row">
      <th scope="row"><%= diary.id %></th>
      <td name= "date" ><%= diary.date %></td>
      <td name= "worker"><%= diary.worker.name %></td>
      <td name= "work" ><%= diary.work.location.name %> - <%= diary.work.name %></td>
      <% if diary.completed_work_size %>
        <td><%= diary.completed_work_size %> <%= diary.work.dimension %></td>  
      <% else %>
        <td>Н/Д</td>
      <% end %>
      <% if diary.hours %>
        <td><%= diary.hours %></td>
      <% else %>
        <td>Н/Д</td>
      <% end %>
      <td><%= link_to "Подробнее", diary_path(diary) %></td>
      </tr>
